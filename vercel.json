{
    "version": 2,
    "builds": [
      {
        "src": "src/",
        "use": "@vercel/static-build",
        "config": {
          "buildCommand": "bash custom-build.sh"
        }
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/.*", "dest": "src/index.html" }
    ]
  }
  